<div class="f5 f4-ns w-100 tc tr-ns ph3-m ph5-l pt3 pb2 pb5-ns v-mid">
    <span class="flex flex-wrap items-center justify-center fr-ns">
        {% for item in nav %}
            {% if item.cta %}
                <a id="{{ item.key }}" class="dib ph3 pv2 mv0 fw6 bg-dark-gray white pointer ba b--transparent br2 no-underline grow" href="{{ item.path }}">{{ item.name }}</a>
            {% else %}
                <a id="{{ item.key }}" class="dib ph3 pv2 bg-dark-gray white fw6 pointer no-underline grow" href="{{ item.path }}">{{ item.name }}</a>
            {% endif %}
        {% endfor %}
    </span>
</div>